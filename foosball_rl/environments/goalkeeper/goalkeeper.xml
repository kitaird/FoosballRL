<mujoco model="kicker">
    <option timestep="0.002" integrator="implicitfast"/>

    <default class="main">
        <joint armature="1" damping="1"/>
        <default class="plastic">
            <default class="figure">
                <geom condim="3" mass="0.046" solimp="0.4 0.45 0.01" solref="-200000 -20" friction="0.15 0.0005 0.0001"
                      contype="2" conaffinity="1" type="mesh" mesh="figure" rgba="0 0 0 1"/>
            </default>
            <default class="stopper">
                <geom condim="3" mass="0.046" solimp="0.4 0.45 0.01" solref="-200000 -20" friction="0.15 0.0005 0.0001"
                      contype="8" conaffinity="1" quat="1 1 0 0" type="cylinder" size="0.015 0.01" rgba="0 0 1 1"/>
            </default>
            <default class="ball">
                <geom condim="6" mass="0.021" solimp="0.2 0.3 0.01" solref="-125000 -85" friction="0.1 0.005 0.002"
                      contype="1" conaffinity="30" priority="1" type="sphere" size="0.0175" rgba="1 0 0 1"/>
            </default>
            <default class="field">
                <geom condim="6" solimp="0.4 0.45 0.01" solref="-200000 -20" friction="0.15 0.0005 0.0001"
                      rgba="1 1 1 1" conaffinity="1"/>
            </default>
            <default class="wall">
                <geom condim="6" solimp="0.4 0.45 0.01" solref="-200000 -20" friction="0.15 0.0005 0.0001"
                      rgba="0.6 0.6 0.6 1" contype="4" conaffinity="1"/>
            </default>
            <default class="griff">
                <geom priority="1" mass="0.104" type="cylinder" size="0.016 0.0595" conaffinity="0"
                      quat="1 1 0 0" rgba="0 0 0 1"/>
            </default>
        </default>
        <default class="metal">
            <geom priority="1" density="7700" condim="3" solref="-250000 -10" friction="0.005 0.00005 0.0001"
                  type="cylinder" size="0.008" rgba="0.5 0.5 0.5 1" contype="16" conaffinity="0"/>
        </default>
        <default class="slide_motor">
            <motor forcelimited="true" forcerange="-26 26" ctrllimited="true" ctrlrange="-1.0 1.0" gear="26"/>
        </default>
        <default class="hinge_motor">
            <motor forcelimited="true" forcerange="-7.4 7.4" ctrllimited="true" ctrlrange="-1.0 1.0" gear="250"/>
        </default>
    </default>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
        <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
                 markrgb="0.8 0.8 0.8" width="300" height="300"/>
        <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>

        <texture name="foosball_field_corner" type="2d" file="../assets/soccer_corner.png"/>
        <texture name="foosball_field" type="2d" file="../assets/soccer_field.png"/>
        <material name="field_corner" texture="foosball_field_corner"/>
        <material name="field" texture="foosball_field"/>

        <mesh name="longWallLeft" file="../assets/long_wall/longWallLeftWO.stl"/>
        <mesh name="longWallLeft1" file="../assets/long_wall/longWallLeft1.stl"/>
        <mesh name="longWallLeft2" file="../assets/long_wall/longWallLeft2.stl"/>
        <mesh name="longWallLeft3" file="../assets/long_wall/longWallLeft3.stl"/>
        <mesh name="longWallLeft4" file="../assets/long_wall/longWallLeft4.stl"/>
        <mesh name="longWallLeft5" file="../assets/long_wall/longWallLeft5.stl"/>
        <mesh name="longWallLeft6" file="../assets/long_wall/longWallLeft6.stl"/>
        <mesh name="longWallLeft7" file="../assets/long_wall/longWallLeft7.stl"/>
        <mesh name="longWallLeft8" file="../assets/long_wall/longWallLeft8.stl"/>
        <mesh name="longWallRight" file="../assets/long_wall/longWallRightWO.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight1" file="../assets/long_wall/longWallRight1.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight2" file="../assets/long_wall/longWallRight2.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight3" file="../assets/long_wall/longWallRight3.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight4" file="../assets/long_wall/longWallRight4.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight5" file="../assets/long_wall/longWallRight5.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight6" file="../assets/long_wall/longWallRight6.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight7" file="../assets/long_wall/longWallRight7.stl" refquat="0 0 0 1"/>
        <mesh name="longWallRight8" file="../assets/long_wall/longWallRight8.stl" refquat="0 0 0 1"/>

        <mesh name="kickerRahmenTop1" file="../assets/kickerRahmenTop1.stl"/>
        <mesh name="kickerRahmenTop2" file="../assets/kickerRahmenTop2.stl"/>

        <mesh name="figure" file="../assets/figure.obj" refquat="1 -1 0 0"/>

        <mesh name="shortWall1" file="../assets/short_wall/shortWall1.stl" refquat="1 1 0 0"/>
        <mesh name="shortWall2-mid" file="../assets/short_wall/shortWall2-mid.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-1" file="../assets/short_wall/shortWall2-1.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-2" file="../assets/short_wall/shortWall2-2.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-3" file="../assets/short_wall/shortWall2-3.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-4" file="../assets/short_wall/shortWall2-4.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-5" file="../assets/short_wall/shortWall2-5.stl" refquat="1 1 0 0"/>
        <mesh name="rightShortWall2-6" file="../assets/short_wall/shortWall2-6.stl" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-1" file="../assets/short_wall/shortWall2-1.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-2" file="../assets/short_wall/shortWall2-2.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-3" file="../assets/short_wall/shortWall2-3.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-4" file="../assets/short_wall/shortWall2-4.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-5" file="../assets/short_wall/shortWall2-5.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="leftShortWall2-6" file="../assets/short_wall/shortWall2-6.stl" scale="1 -1 1" refquat="1 1 0 0"/>
        <mesh name="shortWall3" file="../assets/short_wall/shortWall3.stl" refquat="1 1 0 0"/>
    </asset>

    <visual>
        <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global azimuth="-130" elevation="-20"/>
    </visual>

    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="0 0 -1" directional="true" pos="0 0 1"/>
        <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>
        <camera name="table_view" pos="0 0 1.5" resolution="640 480" xyaxes="1 0 0 0 1 0"/>

        <!-- Table -->
        <body name="table" pos="0 0 0">
            <!-- Table Surface -->

            <body name="field" pos="0 0 0.28">
                <geom type="box" class="field" material="field_corner" pos="0.5375 0.26375 -0.005" size="0.075 0.0775 0.01" euler="3.5 -3.5 0"/>
                <geom type="box" class="field" material="field_corner" pos="-0.5375 0.26375 -0.005" size="0.075 0.0775 0.01" euler="3.5 3.5 0"/>
                <geom type="box" class="field" material="field_corner" pos="-0.5375 -0.26375 -0.005" size="0.075 0.0775 0.01" euler="-3.5 3.5 0"/>
                <geom type="box" class="field" material="field_corner" pos="0.5375 -0.26375 -0.005" size="0.075 0.0775 0.01" euler="-3.5 -3.5 0"/>
                <geom type="box" class="field" material="field" pos="0 0 -0.005" size="0.6085 0.34 0.01"/>
            </body>

            <body name="rahmen_top_left" pos="-0.7 0 0">
                <geom type="mesh" class="wall" mesh="kickerRahmenTop1" conaffinity="0"/>
                <geom type="mesh" class="wall" mesh="kickerRahmenTop2" conaffinity="0"/>
            </body>

            <body name="rahmen_top_right" pos="0.7 0 0" quat="0 0 0 1">
                <geom type="mesh" class="wall" mesh="kickerRahmenTop1" conaffinity="0"/>
                <geom type="mesh" class="wall" mesh="kickerRahmenTop2" conaffinity="0"/>
            </body>

            <body name="long_wall_Left" pos="0 -0.354 0">
                <geom type="mesh" class="wall" mesh="longWallLeft"/>
                <geom type="mesh" class="wall" mesh="longWallLeft1"/>
                <geom type="mesh" class="wall" mesh="longWallLeft2"/>
                <geom type="mesh" class="wall" mesh="longWallLeft3"/>
                <geom type="mesh" class="wall" mesh="longWallLeft4"/>
                <geom type="mesh" class="wall" mesh="longWallLeft5"/>
                <geom type="mesh" class="wall" mesh="longWallLeft6"/>
                <geom type="mesh" class="wall" mesh="longWallLeft7"/>
                <geom type="mesh" class="wall" mesh="longWallLeft8"/>
            </body>

            <body name="long_wall_Right" pos="0 0.354 0">
                <geom type="mesh" class="wall" mesh="longWallRight"/>
                <geom type="mesh" class="wall" mesh="longWallRight1"/>
                <geom type="mesh" class="wall" mesh="longWallRight2"/>
                <geom type="mesh" class="wall" mesh="longWallRight3"/>
                <geom type="mesh" class="wall" mesh="longWallRight4"/>
                <geom type="mesh" class="wall" mesh="longWallRight5"/>
                <geom type="mesh" class="wall" mesh="longWallRight6"/>
                <geom type="mesh" class="wall" mesh="longWallRight7"/>
                <geom type="mesh" class="wall" mesh="longWallRight8"/>
            </body>

            <body name="short_wall_white" pos="0.608 -0.1 0.3305" quat="0 1 0 0">
                <geom type="mesh" class="wall" mesh="shortWall1"/>
                <geom type="mesh" class="wall" mesh="shortWall2-mid" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-1" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-2" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-3" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-4" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-5" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-6" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-1" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-2" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-3" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-4" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-5" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-6" pos="0 -0.1 0"/>
                <geom type="mesh" class="wall" mesh="shortWall3"/>
            </body>

            <body name="short_wall_black" pos="-0.608 0 0.3305" quat="0 0 1 0">
                <geom type="mesh" class="wall" mesh="shortWall1" pos="0 0.1 0"/>
                <geom type="mesh" class="wall" mesh="shortWall2-mid"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-1"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-2"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-3"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-4"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-5"/>
                <geom type="mesh" class="wall" mesh="rightShortWall2-6"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-1"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-2"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-3"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-4"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-5"/>
                <geom type="mesh" class="wall" mesh="leftShortWall2-6"/>
                <geom type="mesh" class="wall" mesh="shortWall3" pos="0 0.1 0"/>
            </body>

            <body name="white_goal" pos="0.608 0 0.3305">
                <geom type="box" pos="0.07525 0 -0.06" size="0.0025 0.15 0.1"/>
                <geom type="box" pos="0.045 0.1475 -0.06" size="0.035 0.0025 0.1"/>
                <geom type="box" pos="0.045 -0.1475 -0.06" size="0.035 0.0025 0.1"/>
                <geom type="box" pos="0.045 0 0.0375" size="0.035 0.15 0.0025"/>
                <geom type="box" pos="0.045 0 -0.1575" size="0.035 0.15 0.0025"/>
                <geom type="box" pos="0.005 0 -0.10275" size="0.005 0.15 0.05725"/>
                <site name="white_goal_site" type="box" pos="0.04 0 -0.06" size="0.04 0.15 0.1" rgba="1 0.5 1 1"/>
            </body>

            <body name="black_goal" pos="-0.608 0 0.3305">
                <geom type="box" pos="-0.07525 0 -0.06" size="0.0025 0.15 0.1"/>
                <geom type="box" pos="-0.045 0.1475 -0.06" size="0.035 0.0025 0.1"/>
                <geom type="box" pos="-0.045 -0.1475 -0.06" size="0.035 0.0025 0.1"/>
                <geom type="box" pos="-0.045 0 0.0375" size="0.035 0.15 0.0025"/>
                <geom type="box" pos="-0.045 0 -0.1575" size="0.035 0.15 0.0025"/>
                <geom type="box" pos="-0.005 0 -0.10275" size="0.005 0.15 0.05725"/>
                <site name="black_goal_site" type="box" pos="-0.04 0 -0.06" size="0.04 0.15 0.1" rgba="1 0.5 1 1"/>
            </body>
        </body>

        <!-- Ball -->
        <!-- qpos: pos (3 dim), quat (4 dim) -->
        <!-- qvel: pos (3 dim), eul (3 dim) -->
        <body name="ball" pos="0 0 0.305">
            <freejoint/>
            <geom class="ball"/>
            <site name="ball_site" pos="0 0 0" size="0.0175" rgba="0 0 0 1"/>
        </body>

        <!-- Black Team -->
        <!-- qpos: lateral (1 dim), angular (1 dim) -->
        <!-- qvel: lateral (1 dim), angular (1 dim) -->
        <body name="b_g" pos="-0.522 0 0.3025">
            <joint name="b_g_slide" type="slide" range="-0.1271 0.1291" axis="0 1 0"/>
            <joint name="b_g_hinge" type="hinge" range="-90 90" axis="0 1 0" pos="0 0 0.0605"/>
            <geom class="griff" pos="0 -0.5775 0.0605"/>
            <geom class="metal"  fromto="0 -0.635 0.0605 0 0.535 0.0605"/>
            <site name="b_g_site" type="cylinder" fromto="0 -0.635 0.0605 0 0.535 0.0605" size="0.008"/>

            <geom class="stopper" mass="0.02" pos="0 -0.183 0.0605" size="0.0125 0.006"/>
            <geom class="stopper" mass="0.02" pos="0 0.183 0.0605" size="0.0125 0.006"/>
            <geom class="stopper" pos="0 -0.199 0.0605"/>
            <geom class="stopper" pos="0 0.199 0.0605"/>

            <geom name="player1_1" class="figure" pos="0 0 0.0445"/>
            <site name="b_g_1" size="0.005" pos="0 0 0.05" rgba="0 0 1 1"/>
        </body>
    </worldbody>

    <actuator>
        <!-- black team actuators -->
        <motor group="1" name="b_g_slide_motor" class="slide_motor" joint="b_g_slide"/>
        <motor group="1" name="b_g_hinge_motor" class="hinge_motor" joint="b_g_hinge"/>
    </actuator>

    <sensor>
        <touch name="white_goal_sensor" site="white_goal_site"/> <!-- 1 dim -->
        <touch name="black_goal_sensor" site="black_goal_site"/> <!-- 1 dim -->
        <velocimeter name="ball_vel_sensor" site="ball_site"/> <!-- 3 dim -->
        <accelerometer name="ball_acc_sensor" site="ball_site"/> <!-- 3 dim -->
        <!-- black team: 4 dims-->
        <jointpos name="b_g_slide_pos_sensor" joint="b_g_slide"/> <!-- 1 dim -->
        <jointvel name="b_g_slide_vel_sensor" joint="b_g_slide"/> <!-- 1 dim -->
        <jointpos name="b_g_hinge_pos_sensor" joint="b_g_hinge"/> <!-- 1 dim -->
        <jointvel name="b_g_hinge_vel_sensor" joint="b_g_hinge"/> <!-- 1 dim -->
    </sensor>

    <keyframe>
        <key name="0" qvel="-1 0 0 0 0 0 0 0"/>
        <key name="1" qpos="0.57606 0.3 0.8075 0 0 0 0 0 0"/>
    </keyframe>
</mujoco>